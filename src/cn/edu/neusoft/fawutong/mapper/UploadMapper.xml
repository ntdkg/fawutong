<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="cn.edu.neusoft.fawutong.mapper.UploadDao">
   <insert id="addNews_admin" parameterType="cn.edu.neusoft.fawutong.domain.News">
      INSERT INTO 
      	tbl_news (ntitle,ndate,ncontext,nsource,nimg,ntype)
	  VALUES 
	  	( #{ntitle}, #{ndate}, #{ncontext}, #{nsource}, #{nimg},#{ntype})
   </insert>
</mapper>
